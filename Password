#include <bits/stdc++.h>
using namespace std;
int main(){
    string n,a,b;
    cout<<"Nhap Password:";
    cin>>n;
    if (n=="ahihi123"){
        cout<<"HAPPY!";
    }
    if (n!="ahihi123"){
        cout<<"Nhap Lai Password:";
        cin>>n;
        if (n=="ahihi123"){
            cout<<"HAPPY";
            return 0;
        }
        if (n!="ahihi123") {
            cout<<"Nhap Lai Password:";
            cin>>n;
        }
        if (n=="ahihi123"){
            cout<<"HAPPY";
            return 0;
        }
        if (n!="ahihi123"){
            cout<<"Nhap Lai Password:";
            cin>>n;
            if (n!="ahihi123") {
                cout<<"Do you want to change password?"<<endl;
                cin>>a;
            }
            if (a!="No"&&a!="Yes"){
                cout<<"ERROR.Please Cofirm Yes or No";
            }
            if (a=="No"){
                return 0;
            }
            if (a=="Yes"){
                cout<<"Nhap New Password:";
                cin>>b;
            }
        }
    }
    n=b;
    cout<<"Nhap Password:";
    cin>>n;
    if (n==b){
        cout<<"HAPPY";
    }
    while (n!=b){
        cout<<"Nhap Lai Password:";
        cin>>n;
        if (n==b){
            cout<<"HAPPY";
            return 0;
        }
        if (n!=b) {
            cout<<"Nhap Lai Password:";
            cin>>n;
        }
        if (n==b){
            cout<<"HAPPY";
            return 0;
        }
        if (n!=b){
            cout<<"Nhap Lai Password:";
            cin>>n;
            if (n!=b) {
                cout<<"Do you want to change password?"<<endl;
                cin>>a;
            }
            if (a!="No"&&a!="Yes"){
                cout<<"ERROR.Please Cofirm Yes or No";
                cin>>a;
            }
            if (a=="No"){
                return 0;
            }
            if (a=="Yes"){
                cout<<"Nhap New Password:";
                cin>>n;
            }
        }
    }
    return 0;
}
